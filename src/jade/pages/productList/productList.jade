main.page__productList
    .header-page__wrap
        ol.breadcrumb
            li
                a(href="#!") Каталог
            li
                a(href="#!") Воблеры
            li.active
                a(href="#!") Challenger X
        .header-page__container.productList__titleBlock
            h1.title__lg Challenger X
            .readmore-wrapper
                .tile__text.readmore-content
                    | Lorem ipsum quisque risus nulla massa commodo ultricies diam
                .readmore-button_wrapper
                    button.readmore-button.item-more.js-readmore_btn
                        | ПОДРОБНЕЕ
        .header-page__container
            form#filtrProduct(action="#!")
                .clearfix.productList__filtrBlock
                    .row
                        .col-md-3.col-sm-6
                            .title__xs Рыбак
                            .form-group
                                .filtr__radio--wrapper
                                    input(type="radio", name="skill", id="skill1" checked)
                                    label(for="skill1") Новичок
                                .filtr__radio--wrapper
                                    input(type="radio", name="skill", id="skill2")
                                    label(for="skill2") Опытный
                        .col-md-8.col-sm-6
                            .title__xs Версия
                            .form-group
                                .filtr__radio--wrapper
                                    input(type="radio", name="version", id="version1" checked)
                                    label(for="version1") Новичок
                                .filtr__radio--wrapper
                                    input(type="radio", name="version", id="version2")
                                    label(for="version2") Опытный
                                .filtr__radio--wrapper
                                    input(type="radio", name="version", id="version3")
                                    label(for="version3") Опытный
                    .row
                        .col-md-3.col-sm-6
                            .title__xs Нагрузка
                            .form-group
                                #input_range-1.input__range
                                #input_range-1_value
                        .col-md-3.col-md-offset-1.col-sm-6
                            .title__xs Длина
                            .form-group
                                #input_range-2.input__range
                        .col-md-3.col-md-offset-1.col-sm-6
                            .title__xs Заглубление
                            .form-group
                                #input_range-3.input__range
                .container.productList__filtrBlock--control
                    .row
                        button.productList__filtrBlock--close.active
                            span ❮
                        button(type="reset", class="productList__filtrBlock--reset") сбросить фильтр
    .container-fluid
        .row
            .tile.tile--double(class=' col-4_8 col-lg-6 col-md-4 col-sm-6 ' data-tileid='2')
                .tile_content.tile-product_wrapper
                    a.tile-product(title="Jer-O Minnow")
                        span.tile-product_marker
                        .tile-product_image-wrapper
                            img.tile-product_image(src="http://cdn.strikepro.ru/article/0000750_0_headimage_thumbnail.png")
                        .tile-product_title
                            | Jer-O Minnow
                    ul.tile-product_group_list
                        - var n = 0;
                            while n < 6
                                li.tile-product_group_item
                                    a.tile-product_group_link(href='/')
                                        | Flex Phantom 90
                                - n++
            .tile(class='col-2_4 col-lg-3 col-md-4 col-sm-6')
                .tile_content.tile-product_wrapper
                    a.tile-product(title="Jer-O Minnow")
                        span.tile-product_marker
                        .tile-product_image-wrapper
                            img.tile-product_image(src="http://cdn.strikepro.ru/article/0000750_0_headimage_thumbnail.png")
                        .tile-product_title
                            | Jer-O Minnow
                    ul.tile-product_group_list
                        - var n = 0;
                            while n < 10
                                li.tile-product_group_item
                                    a.tile-product_group_link(href='/')
                                        | Flex Phantom 90
                                - n++

